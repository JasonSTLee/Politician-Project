SELECT
	DISTINCT(ticker)
FROM
	politician
WHERE
	ticker LIKE 'A%'


SELECT
	*
FROM
	politician
WHERE
	ticker = 'RRX'


UPDATE politician
SET ticker = 'NTR'
WHERE ticker = 'AGU'


UPDATE politician
SET shorter_description = 'Nutrien LTD'
WHERE shorter_description = 'Agrium Inc.'


SELECT
	*
FROM
	politician
WHERE
	shorter_description = 'Agrium Inc.'

SELECT
	*
FROM
	politician
WHERE
	shorter_description = 'Archer Daniels'
ORDER BY
	ticker
	

DELETE FROM politician
WHERE ticker = 'ABCO'